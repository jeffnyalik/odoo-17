<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_hospital_tree_view" model="ir.ui.view">
        <field name="name">hospital.patient.tree</field>
        <field name="model">hospital.patient</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="gender"/>
                <field name="age"/>
                <field name="is_child"/>
            </tree>
        </field>
    </record>
</odoo>